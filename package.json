{
  "name": "front-end-template",
  "type": "module",
  "version": "0.0.0",
  "private": true,
  "engines": {
    "node": ">=18.0.0 <18.19.0"
  },
  "scripts": {
    "dev": "vue-tsc -b && eslint . --fix && vite",
    "build:test": "vue-tsc && vite build --mode test",
    "build:pre": "vue-tsc && vite build --mode pre",
    "build:uat": "vue-tsc && vite build --mode uat",
    "build:prod": "vue-tsc -b && vite build --mode production",
    "build": "vue-tsc -b && vite build --mode production",
    "preview": "vite preview",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prepare": "husky install",
    "cz": "git-cz"
  },
  "dependencies": {
    "@antfu/eslint-config": "^2.21.2",
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@iconify/vue": "^4.1.2",
    "@types/js-cookie": "^3.0.6",
    "@types/node": "^20.14.10",
    "@types/nprogress": "^0.2.3",
    "@vue-macros/reactivity-transform": "0.4.6",
    "@vue/compiler-sfc": "^3.3.0",
    "@vueuse/core": "^10.11.0",
    "axios": "^1.7.2",
    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "dayjs": "^1.11.11",
    "element-plus": "^2.7.6",
    "eslint": "^8.57.0",
    "js-cookie": "^3.0.5",
    "lint-staged": "^15.2.7",
    "lodash-es": "^4.17.21",
    "mitt": "^3.0.1",
    "nprogress": "^0.2.0",
    "pinia": "^2.1.7",
    "postcss": "^8.4.39",
    "prettier": "^3.3.2",
    "sass": "^1.77.6",
    "sass-loader": "^14.2.1",
    "scss": "^0.2.4",
    "scss-loader": "^0.0.1",
    "unplugin-auto-import": "^19.3.0",
    "unplugin-element-plus": "^0.8.0",
    "unplugin-vue-components": "^0.27.2",
    "vue": "^3.4.29",
    "vue-router": "4"
  },
  "devDependencies": {
    "@unocss/postcss": "^0.61.3",
    "@unocss/preset-rem-to-px": "^0.61.3",
    "@unocss/transformer-attributify-jsx": "^0.61.3",
    "@unocss/transformer-attributify-jsx-babel": "^0.61.3",
    "@vitejs/plugin-vue": "^5.0.5",
    "@vitejs/plugin-vue-jsx": "^4.0.0",
    "@vue-macros/volar": "^0.23.1",
    "husky": "^8.0.0",
    "typescript": "^5.2.2",
    "unocss": "^0.61.3",
    "unplugin-vue-macros": "^2.9.5",
    "vite": "^5.3.1",
    "vue-tsc": "^2.0.21"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "eslint --fix"
    ]
  }
}
